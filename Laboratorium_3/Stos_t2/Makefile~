#__start__: obj stos_t1
#	./stos_t1

PROG=a

OBJ=obj/main.o obj/stos.o

CPPFLAGS=-Wall -pedantic -Iinc -I/home/krzysztof/Deskopt/PAMSI/Laboratorium_3/Stos_t1/inc -c
LDFLAGS= -Wall -L/home/krzysztof/Deskopt/PAMSI/Laboratorium_3/Stos_t1/obj

#obj:
#	mkdir obj

$(PROG):$(OBJ)
	g++ ${LDFLAGS} -o $(PROG) $(OBJ) -lrt


obj/main.o: src/main.cpp
	g++ ${CPPFLAGS} -o $@ $^
	
obj/stos.o: src/stos.cpp
	g++ ${CPPFLAGS} -o $@ $^

clean:
	rm -f stos_t1 obj/*